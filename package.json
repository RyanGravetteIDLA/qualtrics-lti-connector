{
  "name": "qualtrics-lti-connector",
  "version": "1.0.0",
  "description": "LTI 1.3 connector between Agilix Buzz and Qualtrics for survey distribution and grade passback",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
